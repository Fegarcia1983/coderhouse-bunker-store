ext(CartContext);\n\n   const [order, setOrder] = useState({\n      product: product,\n      qty:0\n   });\n\n   useEffect(() => {\n      setOrder({\n         ...order,\n         product: product,\n      })\n   }, [product])\n\n   const { qty } = order;\n\n   const addQtyHandler = () => {\n      (qty!==product.stock) &&\n      setOrder({\n         ...order,\n         qty: qty + 1\n      })\n   }\n\n   const subQtyHandler = () => {\n      (qty!==0) &&\n      setOrder({\n         ...order,\n         qty: qty - 1\n      })\n   }\n\n   console.log(order);\n\n   return (\n      <div className=\"fg-counter\">\n         < ItemCount stock={ product.stock } order={ order } addQtyHandler={addQtyHandler} subQtyHandler={subQtyHandler}/>\n         <div>\n            <p>Stock: { product.stock }</p>\n         </div>\n         <div className=\"fg-buyButtonContainer\">\n            {\n               qty>0 &&\n               <button className=\"fg-buyButton\" onClick={()=>addOrder(order)}>I want {qty}!!! </button>\n            }\n         </div>\n      </div>\n   )\n}\n","/Users/federico/Documents/01.3.reactjs-CH/03.ejercicios/bunker-store/src/components/main/cart-container/CartContainer.js",[],"/Users/federico/Documents/01.3.reactjs-CH/03.ejercicios/bunker-store/src/components/main/cart-table/CartTable.js",[],"/Users/federico/Documents/01.3.reactjs-CH/03.ejercicios/bunker-store/src/components/main/cart-table-item/CartTableItem.js",[],"/Users/federico/Documents/01.3.reactjs-CH/03.ejercicios/bunker-store/src/components/main/cart-delete-button/CartDeleteButton.js",[],"/Users/federico/Documents/01.3.reactjs-CH/03.ejercicios/bunker-store/src/components/main/cart-change-qty/CartChangeQty.js",[],"/Users/federico/Documents/01.3.reactjs-CH/03.ejercicios/bunker-store/src/firebase/index.js",[],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":45,"column":7,"nodeType":"103","endLine":45,"endColumn":9,"suggestions":"104"},{"r