)\n         }\n         let products = (querySnapshot.docs.map(doc => {\n            return({\n               id: doc.id,\n               ...doc.data()\n            })\n         }))\n         setState( {\n            ...state,\n            products: products\n         } )\n      }).catch((error) => {\n         console.log('Error searching items', error);\n      })\n      return () => {\n         setState( {\n            products: []\n         } )\n      }\n   }, [categoryId])\n\n   return (\n      <div className='fg-mainBody'>\n         < ItemList products={ products }/>\n      </div>\n   )\n}\n","/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/item-detail-container/ItemDetailContainer.js",["99","100"],"import React, { useState, useEffect } from 'react'\nimport { useParams } from 'react-router-dom'\nimport { ItemDetail } from '../item-detail/ItemDetail'\nimport { getFirestore } from '../../../firebase'\nimport './itemDetailContainer.css'\n\nexport const ItemDetailContainer = () => {\n\n   const [state, setState ] = useState( {\n      product: []\n   } )\n\n   const { product } = state\n\n   const { id:searchedId } = useParams()\n\n   useEffect( () => {\n\n      const db = getFirestore();\n      const item = db.collection('items').doc(searchedId)\n      item.get()\n      .then((doc) => {\n         if( !doc.exists ) {\n            console.log('No results!')\n            return\n         }\n         setState( {\n            id: doc.id,\n            ...doc.data()\n         } )\n      }).catch((error) => {\n         console.log('Error searching the product', error);\n      })\n   }, [])\n\n   return (\n      <div className=\"fg-mainBody\">\n         < ItemDetail product={state} />\n      </div>\n   )\n}\n","/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/cart-container/CartContainer.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/navbar/user-widget/UserWidget.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/navbar/menu-button/MenuButton.js",["101"],"import React, { useState, useEffect } from 'react'\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faBars } from '@fortawesome/free-solid-svg-icons'\nimport { Dropdown } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport { getFirestore } from '../../../firebase'\nimport './menubutton.css'\n\nexport const MenuButton = () => {\n   const menuIcon = <FontAwesomeIcon icon={faBars} color='#E9E9E9E9'/>\n\n   const [state, setState] = useState( {\n      categories: []\n   } )\n\n   const {categories} = state\n\n   useEffect( () => {\n\n      const db = getFirestore();\n      const itemCollection = db.collection('categories')\n      itemCollection.get()\n      .then((querySnapshot) => {\n         if( querySnapshot.size === 0 ) {\n            console.log('No results!')\n         }\n         let categories = (querySnapshot.docs.map(doc => {\n            return({\n               id: doc.id,\n               ...doc.data()\n            })\n         }))\n         setState( {\n            ...state,\n            categories: categories\n         } )\n      }).catch((error) => {\n         console.log('Error searching categories', error);\n      })\n      return () => {\n         setState( {\n            categories: []\n         } )\n      }\n   }, [])\n\n   return (\n      <div className=\"dropdown\">\n         <div className=\"dropbtn\"><span>{menuIcon}</span>MENU</div>\n            <div className=\"dropdown-content\">\n               <Link to={'/'}>home</Link>\n               <Link to={'/products'}>productos</Link>\n               <Link to={'/us'}>nosotros</Link>\n               <Dropdown.Divider />\n               {\n                  categories.map( cat =>\n                     <Link\n                     key={cat.id}\n                     to={`/category/${cat.id}`}\n                     >{cat.name}\n                     </Link>\n                  )\n\n               }\n            </div>\n      </div>\n   )\n   }\n","/home/federico/Proyectos/coderhouse-bunker-store/src/components/navbar/cart-widget/CartWidget.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/item-list/ItemList.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/item-detail/ItemDetail.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/buy-form/BuyForm.js",["102","103"],"import React, { useContext, useEffect, useState } from 'react'\nimport { CartContext } from '../../../contexts/CartContext'\nimport { getFirestore } from '../../../firebase'\nimport firebase from 'firebase/app'\n\n\nexport const BuyForm = () => {\n\n   const [info, setInfo] = useState({\n      name: \"\",\n      phone: \"\",\n      mail: \"\"\n   });\n   const [ order, setOrder ] = useState({})\n   const [ orderId, setOrderId ] = useState('')\n\n   const { cart, totalInCart } = useContext(CartContext)\n\n   const { name, phone, mail } = info;\n\n   useEffect(() => {\n      setOrder({\n         buyer: info,\n         items: cart,\n         date: firebase.firestore.Timestamp.fromDate(new Date()),\n         total: totalInCart\n      })\n   }, [totalInCart, info])\n\n   function handleChange(e) {\n   setInfo({\n      ...info,\n      [e.target.name]: e.target.value\n   });\n   }\n\n   function handleSubmit(e) {\n      e.preventDefault()\n      const db = getFirestore()\n      const orderDb = db.collection('orders')\n      orderDb.add(order)\n         .then(({id}) => {\n            setOrderId(id)\n            order.items.forEach( item => {\n               const itemDb = db.collection('items')\n               let documento = itemDb.doc(item.product.id)\n               let actualStock = item.product.stock - item.qty\n               documento.update({stock: actualStock})\n            })\n         })\n         .catch((error) => console.log('Error: ',error))\n   }\n\n   console.log(order);\n\n   return (\n      <form className=\"fg-buyForm\">\n         <h3>Finaliza tu compra</h3>\n         <hr/>\n         <div className=\"fg-buyFormInfo\">\n            <label htmlFor=\"name\">Nombre</label>\n            <input name=\"name\" type=\"text\" onChange={ handleChange }></input>\n            <label htmlFor=\"phone\">Tel√©fono</label>\n            <input name=\"phone\" type=\"text\" onChange={ handleChange }></input>\n            <label htmlFor=\"mail\">Mail</label>\n            <input name=\"mail\" type=\"text\" onChange={ handleChange }></input>\n         </div>\n         <button\n         disabled={\n            !( name && phone && mail )\n          }\n          onClick={ handleSubmit }\n          >Let's do it</button>\n         <p hidden={\n            ( name && phone && mail )\n          }>Completa los datos para poder efectuar la compra</p>\n      </form>\n   )\n}\n","/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/cart-table/CartTable.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/firebase/index.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/item/Item.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/item-count-container/ItemCountContainer.js",["104"],"import React, { useContext, useEffect, useState } from 'react'\nimport { ItemCount } from '../item-count/ItemCount'\nimport { CartContext } from '../../../contexts/CartContext'\nimport './itemCountContainer.css'\n\nexport const ItemCountContainer = ( { product } ) => {\n\n   const { addOrder } = useContext(CartContext);\n\n   const [order, setOrder] = useState({\n      product: product,\n      qty:0\n   });\n\n   useEffect(() => {\n      setOrder({\n         ...order,\n         product: product,\n      })\n   }, [product])\n\n   const { qty } = order;\n\n   const addQtyHandler = () => {\n      (qty!==product.stock) &&\n      setOrder({\n         ...order,\n         qty: qty + 1\n      })\n   }\n\n   const subQtyHandler = () => {\n      (qty!==0) &&\n      setOrder({\n         ...order,\n         qty: qty - 1\n      })\n   }\n\n   return (\n      <div className=\"fg-counter\">\n         < ItemCount stock={ product.stock } order={ order } addQtyHandler={addQtyHandler} subQtyHandler={subQtyHandler}/>\n         <div>\n            <p>Stock: { product.stock }</p>\n         </div>\n         <div className=\"fg-buyButtonContainer\">\n            {\n               qty>0 &&\n               <button className=\"fg-buyButton\" onClick={()=>addOrder(order)}>I want {qty}!!! </button>\n            }\n         </div>\n      </div>\n   )\n}\n","/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/cart-table-item/CartTableItem.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/item-count/ItemCount.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/cart-delete-button/CartDeleteButton.js",[],"/home/federico/Proyectos/coderhouse-bunker-store/src/components/main/cart-change-qty/CartChangeQty.js",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":44,"column":7,"nodeType":"111","endLine":44,"endColumn":19,"suggestions":"112"},{"ruleId":"113","severity":1,"message":"114","line":13,"column":12,"nodeType":"115","messageId":"116","endLine":13,"endColumn":19},{"ruleId":"109","severity":1,"message":"117","line":34,"column":7,"nodeType":"111","endLine":34,"endColumn":9,"suggestions":"118"},{"ruleId":"109","severity":1,"message":"110","line":45,"column":7,"nodeType":"111","endLine":45,"endColumn":9,"suggestions":"119"},{"ruleId":"113","severity":1,"message":"120","line":15,"column":12,"nodeType":"115","messageId":"116","endLine":15,"endColumn":19},{"ruleId":"109","severity":1,"message":"121","line":28,"column":7,"nodeType":"111","endLine":28,"endColumn":26,"suggestions":"122"},{"ruleId":"109","severity":1,"message":"123","line":20,"column":7,"nodeType":"111","endLine":20,"endColumn":16,"suggestions":"124"